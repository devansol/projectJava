	<subDataset name="nilai_matpel">
		<queryString language="SQL">
			<![CDATA[SELECT a.nis, a.nama_siswa, b.uts,b.uas,b.nilai_kehadiran, c.nama_matpel, b.tahun_ajaran 
FROM nilai_siswa.siswa a,nilai_siswa.nilai b,nilai_siswa.matpel c 
WHERE a.nis = b.nis AND b.id_matpel = c.id_matpel]]>
		</queryString>
		<field name="nis" class="java.lang.String"/>
		<field name="nama_siswa" class="java.lang.String"/>
		<field name="uts" class="java.lang.Integer"/>
		<field name="uas" class="java.lang.Integer"/>
		<field name="nilai_kehadiran" class="java.lang.Integer"/>
		<field name="nama_matpel" class="java.lang.String"/>
		<field name="tahun_ajaran" class="java.lang.String"/>
	</subDataset>
	<parameter name="nis" class="java.lang.String">
		<defaultValueExpression><![CDATA[$F{nis}]]></defaultValueExpression>
	</parameter>
	<parameter name="tahun_ajaran" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="semester" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT a.nis,a.nama_siswa,a.kelas,b.uts,b.uas,b.tugas, c.nama_matpel,b.tahun_ajaran,round( ( b.uts + b.uas + b.tugas ) /3 ) AS total,(select sum(round( ( bb.uts + bb.uas + bb.tugas ) /3 )) from nilai_siswa.nilai bb,nilai_siswa.siswa a,nilai_siswa.matpel c  where bb.nis = a.nis and bb.kode_matpel = c.kode_matpel) as jumlah FROM nilai_siswa.siswa a,
	nilai_siswa.nilai b,
	nilai_siswa.matpel c 
WHERE a.nis = b.nis 
	AND b.kode_matpel = c.kode_matpel 
	AND a.nis = $P{nis}
	AND b.tahun_ajaran = $P{tahun_ajaran}
	AND b.semester = $P{semester}       



]]>
	</queryString>
